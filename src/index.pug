//- Подключил шаблон с разметкой страницы
extends doctypes.pug

//- Изменил название страницы
block doctype
    //- Описание страницы в head discription
    - var contentDiscription = 'Shneider Finder Phenix Seimens';
    //- Наименование главной страницы
    - var titleName = 'Интернет-магазин коплектующих для НКУ';
    //- Массив с путями к файлам со стилями
    - var stylePath = ['css/ip-style.css'];
    //-- указывает поисковому роботу адрес главной страницы среди группы страниц дублей
    - var canonical = '';


//- Вставил блок с будующим контентом
block content
    include html/header.pug 
    main
        h1(class="visually-hidden h1-visually", style="visibility: hidden;").h1__visually Интернет-магазин комплектующих НКУ
        
    script(src="js/app.js", type="module")
    